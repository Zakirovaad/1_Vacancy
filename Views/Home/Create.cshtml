@model Vacancy.Models.VacancyList
@{
    ViewBag.Title = "Добавление вакансии";
}
<form asp-action="create" asp-controller="home">
    <!-- <div class="form-group">
         <label asp-for="id" class="control-label">Id</label>
         <input type="number" asp-for="id" class="form-control" />
     </div> -->
    <div class="form-group">
        <label asp-for="name" class="control-label">Заголовок</label>
        <input type="text" asp-for="name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="salary" class="control-label">Оклад</label>
        <input type="number" asp-for="salary" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="organization" class="control-label">Организация</label>
        <input type="text" asp-for="organization" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="contact_person" class="control-label">Контактное лицо</label>
        <input type="text" asp-for="contact_person" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="phone_number" class="control-label">Телефон</label>
        <input type="text" asp-for="phone_number" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="empl_type" class="control-label">Тип занятости</label>
        <input type="text" asp-for="empl_type" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="description" class="control-label">Описание вакансии</label>
        <textarea rows="10" asp-for="description" class="form-control" ></textarea>
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
